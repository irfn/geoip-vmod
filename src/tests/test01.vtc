varnishtest "Test GeoIP vmod"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import geoip from "${pwd}/.libs/libvmod_geoip.so";

	sub vcl_deliver {
		set resp.http.X-GeoIP = geoip.country("127.0.0.1");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.X-GeoIP == "Unknown"
}

client c1 -run
